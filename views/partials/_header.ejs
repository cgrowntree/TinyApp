<!-- views/partials/_header.ejs -->
<header>
  <H1>Tiny App</H1>
  <% if (user_id) { %>
    <form action="/logout" method="POST" type="submit" value="Logout">
      <span><%= user_id %></span>
    <button>Logout</button>
    </form>
  <% } else{ %>
    <a href="/login">Login</a>
  <% } %>
</header>
