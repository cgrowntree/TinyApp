<!-- views/urls_index.ejs -->
<!DOCTYPE html>
<html lang="en">
  <head>
    <% include partials/_header %>
  </head>
  <body>
    <ul>
      <% Object.keys(urls).forEach(function(key) { %>
        <li><%= key %> --> <%= urls[key] %><form method="POST" action="/urls/<%= key %>/delete"><button type='submit'>DELETE</button></form></li>
      <% }); %>
    </ul>
    <a href="/urls/new">Create new short URL here</a>
  </body>
</html>